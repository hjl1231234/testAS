            <intent-filter>
                <action android:name="com.example.testas.3" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Main2Activity"
            android:label="this second">
            <intent-filter>
                <action android:name="com.example.testas.2" />
                <category android:name="android.intent.category.DEFAULT" />
                <!-- 新添加的category -->
                <!-- <category android:name="com.example.testas.MY_CATEGORY" /> -->
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="This	is	FirstActivity">


            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
