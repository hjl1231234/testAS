        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Main5Activity"
            android:label="this fifth">

            <intent-filter>
                <action android:name="com.example.testas.5" />
